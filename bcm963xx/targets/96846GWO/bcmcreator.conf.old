BRCM_ROOTFS_CONFIG=y
BRCM_KERNEL_ROOTFS=ubifs
BRCM_ROOTFS_RW=y
BUILD_MTDUTILS=y
BRCM_NFS_MOUNT_EN=y
BRCM_FLASH_BUILD_NAND=y
